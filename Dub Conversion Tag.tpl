___SANDBOXED_JS_FOR_SERVER___

// Require necessary APIs
const sendHttpRequest = require('sendHttpRequest');
const getEventData = require('getEventData');
const logToConsole = require('logToConsole');
const getType = require('getType');
const makeString = require('makeString');
const makeNumber = require('makeNumber');
const getTimestampMillis = require('getTimestampMillis');
const JSON = require('JSON');

// Log the message
function log(message, data) {
  if (enableLogging) {
    logToConsole('[Dub Tag] ' + message, data);
  }
}

// Get configuration from template
const enableLogging = data.enableLogging || false;
const apiKey = data.apiKey;

const event = getEventData('event');
const endpoint = 'https://api.dub.co/track/' + event;
const payload = {};

const eventFieldMap = {
  lead: ['clickId', 'customerExternalId', 'eventName', 'customerName', 'customerEmail', 'customerAvatar', 'eventQuantity', 'mode', 'metadata'],
  sale: ['customerExternalId', 'eventName', 'amount', 'paymentProcessor', 'currency', 'invoiceId', 'leadEventName', 'metadata']
};

const eventFields = eventFieldMap[event] || [];

eventFields.forEach((field) => {
  if (field === 'eventQuantity' || field === 'amount') {
    payload[field] = makeNumber(data[field]);
  } else if (data[field] !== undefined) {
    payload[field] = data[field];
  }
});

const requestOptions = {
  method: 'POST',
  timeout: 5000,
  headers: {
    'Authorization': 'Bearer ' + apiKey,
    'Content-Type': 'application/json',
    'User-Agent': 'GTM-Server/1.0'
  },
};

const requestBody = JSON.stringify(payload);

log('Request payload', payload);

// Send the request
sendHttpRequest(endpoint, requestOptions, requestBody).then(function(response) {
  log('Response', response.body);

  if (response.statusCode >= 200 && response.statusCode < 300) {
    log('Success');
    data.gtmOnSuccess();
  } else {
    log('Error');
    data.gtmOnFailure();
  }
}, function(error) {
  log('Request failed:', error);
  data.gtmOnFailure();
});


